<?xml version="1.0" encoding="utf-8"?>
<resources>


	<declare-styleable name="CircularMeter">

        <attr name="typeface" format="string" />
        <attr name="metricSize" format="dimension" />
        <attr name="metricText" format="string" />
        <attr name="range" format="integer" />
    </declare-styleable>

    <declare-styleable name="RoundedCornerImageView">
        <attr name="cornerRadius" format="dimension" />
        <attr name="cornerColor" format="color" />
        <attr name="cornerLeftDisable" format="boolean" />
        <attr name="cornerTopDisable" format="boolean" />
        <attr name="cornerRightDisable" format="boolean" />
        <attr name="cornerBottomDisable" format="boolean" />
    </declare-styleable>
        <declare-styleable name="CircleView">
                <attr name="progressCurrentValue" format="float"/>
                <attr name="progressMaximumValue" format="float"/>
                <attr name="progressMinimumValue" format="float"/>
                <attr name="progressStepValue" format="float"/>
                <attr name="progressStepAsInteger" format="boolean"/>
                <attr name="progressWidth" format="dimension"/>
                <attr name="progressColor" format="color"/>
                <attr name="progressBarSquared" format="boolean"/>

                <attr name="hasIndicator" format="boolean"/>
                <attr name="indicatorColor" format="color"/>
                <attr name="indicatorRadius" format="integer"/>

                <attr name="arcWidth" format="dimension"/>
                <attr name="arcColor" format="color"/>
                <attr name="arcHasBorder" format="boolean"/>
                <attr name="arcBorderColor" format="color"/>

                <attr name="textEnabled" format="boolean"/>
                <attr name="textSize" format="dimension"/>
                <attr name="textColor" format="color"/>
                <attr name="textDecimalPlaces" format="integer"/>
                <attr name="textFont" format="string"/>

                <attr name="suffixEnabled" format="boolean"/>
                <attr name="suffixValue" format="string"/>

                <attr name="clockwise" format="boolean"/>
                <attr name="enabled" format="boolean"/>
        </declare-styleable>



        <declare-styleable name="RTCGlView">
                <attr name="mainMirror" format="boolean" />
                <attr name="secondMirror" format="boolean" />
                <attr name="mainCoords" format="reference" />
                <attr name="secondCoords" format="reference" />
        </declare-styleable>

        <declare-styleable name="SeekBarPreference">
                <attr name="min" format="integer" />

        </declare-styleable>

        <declare-styleable name="MaskedImageView">
                <attr name="mask" format="integer" />
        </declare-styleable>

        <declare-styleable name="MessageTextView">
                <attr name="widget_id_top" format="integer" />
                <attr name="widget_id_bottom" format="integer" />
                <attr name="widget_id_link_preview" format="integer" />
        </declare-styleable>
</resources>